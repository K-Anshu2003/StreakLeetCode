//Vowels game in a string

class Solution {
    public boolean doesAliceWin(String s) {

        int numOfVowels = 0;
        for(char c : s.toCharArray()){
            if(c=='a' || c=='e' || c=='i' || c=='o' || c=='u') numOfVowels++;
        }

        if(numOfVowels == 0){
            return false;
        }

        return true;  
    }
}
