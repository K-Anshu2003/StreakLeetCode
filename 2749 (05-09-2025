class Solution {
    public int makeTheIntegerZero(int num1, int num2) {
        for (int k = 1; k <= 60; k++) {
            long x = (long) num1 - (long) k * num2;
            if (x < 0) break;

            // Count number of 1s in binary representation of x
            int bitCount = Long.bitCount(x);

            // We need at least 'bitCount' operations to remove all 1s
            // and at most 'k' operations available
            if (bitCount <= k && k <= x) {
                return k;
            }
        }
        return -1;
    }
}
